
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Pal RSA Portal</title>
    @Scripts.Render("~/bundles/jquery")
    @*@Scripts.Render("~/bundles/jqueryval")*@
    @Styles.Render("~/Content/css")

    <link href="~/Content/datepicker.css" rel="stylesheet" />
    <link href="//cdn.datatables.net/1.10.10/css/dataTables.bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/jquery.fancybox.css" rel="stylesheet" />

    @Scripts.Render("~/bundles/modernizr")
    @Styles.Render("~/Content/admin.css")
    @Styles.Render("~/Content/template.css")
    @Styles.Render("~/font-awesome-4.2.0/css/font-awesome.css")
    <script type="text/javascript" src="//cdn.datatables.net/1.10.10/js/jquery.dataTables.min.js"></script>
    <script type="text/javascript" src="//cdn.datatables.net/1.10.10/js/dataTables.bootstrap.min.js"></script>
    <script type="text/javascript" src="~/Scripts/apply.js"></script>
    <script src="@Url.Content("~/Scripts/bootstrap-datepicker.js")" type="text/javascript"></script>
    <script type="text/javascript" src="~/Scripts/devteam.js"></script>
    <script type="text/javascript" src="~/Scripts/jquery.fancybox.js"></script>



    <script type="text/javascript">
    $(document).ready(function () {
        $(".admin_nav_icon").click(function () {
            $(".login_details").slideToggle("fast");
        });
    });
    </script>

</head>
<body>
    @Html.Partial("_TopPlain")
    <div id="main" class="container-fluid">
        <div class="row">
            <!--Start Content-->
            <div id="content">

                @RenderBody()

            </div><!--End Content-->
        </div>
        <footer class="footer-info">
            <p class="text-center">PAL Pensions RSA &copy; @DateTime.Now.Year</p>
        </footer>
    </div>
    @RenderSection("scripts", required: false)
</body>
</html>